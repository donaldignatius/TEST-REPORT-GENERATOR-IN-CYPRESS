{
  "name": "cypress-test-report-generator-framework2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "pretest": "rimraf -rf ./cypress/reports/mochawesome.json",
    "test": "npm run cypress-test || npm run posttest",
    "cypress-test": "cypress run",
    "posttest": "npm run merge-reports && npm run generate-htmlreport",
    "merge-reports": "mochawesome-merge ./cypress/reports/*.json > ./report1.json",
    "generate-htmlreport": "marge ./report1.json --reportDir ./cypress/reports"
  },
  "author": "Donald Ignatius",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^13.3.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "rimraf": "^5.0.5"
  }
}
